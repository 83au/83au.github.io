{
  "name": "beaushermanportfolio",
  "version": "1.0.0",
  "description": "Portfolio website for Beau Sherman",
  "main": "script.js",
  "scripts": {
    "watch:sass": "sass --watch dist/resources/scss/main.scss dist/resources/css/main.css",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "sass dist/resources/scss/main.scss dist/resources/css/main.css",
    "prefix:css": "postcss --use autoprefixer -c 'last 10 versions' dist/resources/css/main.css -o dist/resources/css/main.prefix.css",
    "compress:css": "node-sass dist/resources/css/main.prefix.css dist/resources/css/main.min.css --output-style compressed",
    "bundle": "webpack",
    "build": "npm-run-all compile:sass prefix:css compress:css bundle"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/83au/83au.github.io.git"
  },
  "author": "Beau Sherman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/83au/83au.github.io/issues"
  },
  "homepage": "https://github.com/83au/83au.github.io#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "autoprefixer": "^9.8.0",
    "babel-loader": "^8.1.0",
    "html-webpack-plugin": "^4.3.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
